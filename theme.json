		"custom": {
			"color": {
				"gray-950": "hsl(0 0% 3.9%)",
				"gray-900": "hsl(0 0% 9%)",
				"gray-700": "hsl(0 0% 25.1%)",
				"gray-500": "hsl(0 0% 45.1%)",
				"gray-300": "hsl(0 0% 83.1%)",
				"gray-100": "hsl(0 0% 96.1%)"
			},
			"mod": {
				"darken-amount": "20%"
			},
			"transition": {
				"timing-function": "cubic-bezier(0.4, 0, 0.2, 1)",
				"duration": ".15s"
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--custom--color--gray-950)",
			"text": "var(--wp--preset--color--white)"
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"right": "1.5rem",
				"left": "1.5rem"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--geist)",
			"fontWeight": "400",
			"fontSize": "1rem",
			"lineHeight": "1.75rem"
		},
		"elements": {
			"link": {
				"color": {
					"text": "currentColor"
				},
				":hover": {
					"color": {
						"text": "color-mix(in hsl decreasing hue, currentColor 100%, var(--wp--preset--color--black) var(--wp--custom--mod--darken-amount))"
					}
				}
			},
			"button": {
				"color": {
					"text": "var(--wp--custom--color--gray-900)",
					"background": "var(--wp--preset--color--white)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--custom--color--gray-900)",
						"background": "color-mix(in hsl decreasing hue, var(--wp--preset--color--white) 100%, var(--wp--preset--color--black) var(--wp--custom--mod--darken-amount))"
					}
				},
				"border": {
					"radius": "var(--wp--preset--border-radius--xxs)"
				},
				"spacing": {
					"padding": {
						"top": "8px",
						"right": "16px",
						"bottom": "8px",
						"left": "16px"
					}
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--geist)",
					"fontWeight": "400"
				}
			},
			"h1": {
