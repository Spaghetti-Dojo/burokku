{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1680px"
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "background",
					"color": "hsl(0, 0%, 3.9%)",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "hsl(0, 0%, 98%)",
					"name": "Foreground"
				},
				{
					"slug": "primary",
					"color": "hsl(0, 0%, 98%)",
					"name": "Primary"
				},
				{
					"slug": "primary-foreground",
					"color": "hsl(0, 0%, 9%)",
					"name": "Primary Foreground"
				},
				{
					"slug": "muted",
					"color": "hsl(0, 0%, 14.9%)",
					"name": "Muted"
				},
				{
					"slug": "muted-foreground",
					"color": "hsl(0, 0%, 63.9%)",
					"name": "Muted Foreground"
				},
				{
					"slug": "accent",
					"color": "hsl(0, 0%, 14.9%)",
					"name": "Accent"
				},
				{
					"slug": "accent-foreground",
					"color": "hsl(0, 0%, 98%)",
					"name": "Accent Foreground"
				},
				{
					"slug": "destructive",
					"color": "hsl(0, 62.8%, 30.6%)",
					"name": "Destructive"
				},
				{
					"slug": "destructive-foreground",
					"color": "hsl(0, 0%, 98%)",
					"name": "Destructive Foreground"
				},
				{
					"slug": "border",
					"color": "hsl(0, 0%, 14.9%)",
					"name": "Border"
				},
				{
					"slug": "input",
					"color": "hsl(0, 0%, 14.9%)",
					"name": "Input"
				},
				{
					"slug": "ring",
					"color": "hsl(0, 0%, 83.1%)",
					"name": "Ring"
				}
			]
		},
		"typography": {
			"customFontSize": true,
			"fontSizes": [],
			"fontFamilies": []
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			],
			"padding": true,
			"margin": true
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"spacing": {
			"blockGap": "1.5rem"
		}
	}
}
