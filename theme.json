{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1680px"
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"palette": []
		},
		"typography": {
			"custom": true,
			"customFontSize": true,
			"fontSizes": [],
			"fontFamilies": []
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			],
			"padding": true,
			"margin": true
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "1.5rem"
		}
	},
	"customProperties": {}
}
